{
    "coc.source.gocode.enable": false,
    "languageserver": {
        "golang": {
            "command": "go-langserver",
            "filetypes": ["go"],
            "initializationOptions": {
                "funcSnippetEnabled": true,
                "lintTool": "golint",
                "gocodeCompletionEnabled": true,
                "diagnosticsEnabled": true
            }
        },
        "clangd": {
            "command": "clangd",
            "filetypes": ["c", "cpp", "objc", "objcpp"]
        }
    }
}
